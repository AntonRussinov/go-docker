{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
        {
            "name":"testexercise",
            "image":"arussinov/testexercise",
            "hostname":"testexercise",
            "essential":false,
            "memory" : 128
        },
        {
            "name":"nginx",
            "image":"arussinov/nginx",
            "hostname":"nginx",
            "essential":true,
            "memory" : 128,
            "portMappring":[
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
            "links":["testexercise"],
            "mountPoints":[
                {
                "sourceVolume": "awseb-logs-nginx",
                "containerPath": "/var/log/nginx"
                }
            ]
        }
    ]
}